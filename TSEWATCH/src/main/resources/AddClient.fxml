<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="130.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.AddClientController">
   <children>
      <VBox layoutX="23.0" layoutY="14.0" spacing="10.0">
         <children>
            <HBox>
               <children>
                  <Label prefHeight="27.0" prefWidth="97.0" text="Nom du client : ">
                     <effect>
                        <Blend />
                     </effect></Label>
                  <TextField fx:id="clientText" />
               </children>
            </HBox>
            <HBox>
               <children>
                  <Label prefHeight="27.0" prefWidth="98.0" text="Adresse Email : ">
                     <effect>
                        <Blend />
                     </effect></Label>
                  <TextField fx:id="emailText" />
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox layoutX="156.0" layoutY="89.0" spacing="20.0">
         <children>
            <Button fx:id="ACAnnulerButton" cancelButton="true" mnemonicParsing="false" onAction="#ACAnnulerButtonClicked" text="Annuler" />
            <Button fx:id="ACAjouterButton" defaultButton="true" mnemonicParsing="false" text="Ajouter" />
         </children>
      </HBox>
   </children>
</AnchorPane>
